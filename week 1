Q12. Descending Order of 4 Numbers

import java.util.Arrays;
import java.util.Scanner;
public class DescendingOrder {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int[] arr = new int[4];
        for (int i = 0; i < 4; i++) {
            arr[i] = sc.nextInt();
        }
        Arrays.sort(arr);
        for (int i = 3; i >= 0; i--) {
            System.out.print(arr[i] + " ");
        }
        sc.close();
    }
}
