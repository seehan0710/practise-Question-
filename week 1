Q18. Print Digits Right to Left

import java.util.Scanner;
public class PrintDigits {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        while (n > 0) {
            int digit = n % 10;   
            System.out.print(digit + " ");
            n /= 10;             
        }
        sc.close();
    }
}
