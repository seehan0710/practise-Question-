Q20. Count Digits

import java.util.Scanner;
public class DigitCount {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        n = Math.abs(n);
        int count = 0;
        if (n == 0) {
            count = 1; // Special case: 0 has 1 digit
        } else {
            while (n > 0) {
                count++;
                n /= 10;
            }
        }
        System.out.println(count);
        sc.close();
    }
}
